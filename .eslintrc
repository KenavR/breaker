{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "ecmaFeatures": {
    "jsx": true,
    "es6": true,
    "classes": true
  },
  "rules": {
    "max-len": [1, 180, 4],
    "arrow-body-style": [0],
    "new-cap": 0,
    "comma-dangle": 0,
    "react/prop-types": 0,
    "react/jsx-indent-props": 0
  }
}
